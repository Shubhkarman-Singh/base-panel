 <script>
   document.addEventListener("DOMContentLoaded", function() {
     var currentPagePath = window.location.pathname;
     var navLinks2 = document.querySelectorAll('.nav-link2');
     navLinks2.forEach(function(link) {
       var linkPath = new URL(link.getAttribute('href'), window.location.origin).pathname;
       // Handle default route - if we're on /admin/misc, highlight analytics tab
       if ((currentPagePath === linkPath) || 
           (currentPagePath === '/admin/misc' && linkPath === '/admin/misc/analytics')) {
         link.classList.remove('nav-link2');
         link.classList.add('text-white', 'font-medium', 'border', 'border-transparent', 'bg-white/10', 'py-2', 'px-3', 'rounded-xl', 'shadow-sm');
       }
     });
   });
   </script>